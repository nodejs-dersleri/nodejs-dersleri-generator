doctype html
block vars
  - var bodyclass = null;
html(lang='en')
  head
    block head
      meta(charset='utf-8')
      meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
      meta(name='viewport', content='width=device-width')
      title
        block title
          = locals.name
          
      // begin :og block
      
      block ogMeta
      
      - var quid = '?v='+ (Math.random().toString(16)+"000000000").substr(2,8)  
    
        
      // begin :og block
          
      link(rel='alternate', href=locals.url+'/feed.xml', type='application/rss+xml', title=locals.description)
      link(rel='stylesheet', href='https://fonts.googleapis.com/css?family=Raleway:800')
      
      link(rel='stylesheet', href='/css/normalize.css'+quid)
      link(rel='stylesheet', href='/lib/highlight/styles/github.css'+quid)
      link(rel='stylesheet', href='/fonts/octicons/octicons.css'+quid)
      link(rel='stylesheet', href='/fonts/socicon/socicon.css'+quid)
      link(rel='stylesheet', href='/css/main.css'+quid)

  body(class=bodyclass)
    
    // Begin: Aside
    aside#particles-js
      h1
        a(href=locals.url)= locals.name
      p= locals.description
    // End: Aside
    
    // Begin: Content
    main(role=main) 
      include partials/menu
      block content
      // Begin: Footer
      footer
        block footer
          span.made-with-heart
            span.octicon.octicon-pencil
            |  with 
            span.octicon.octicon-heart 
            |  by  
            a(href='http://www.github.com/arifcakiroglu') Arif Cakiroglu
            
            
      
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js')
    script(src='/javascript/particles.min.js'+quid)
    script(src='/javascript/canvas.js'+quid)
    
    
    script(src='/lib/highlight/highlight.pack.js'+quid)
    script.
      $(document).ready(function() {
        $('pre code').each(function(i, block) {
          hljs.highlightBlock(block);
        });
      });
    script.
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-69517099-1', 'auto');
      ga('send', 'pageview');
          
          
